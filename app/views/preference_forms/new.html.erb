<h1 class="my-4">New Form</h1>
<%= form_for(@form) do |f| %>
    <%= render(:partial => "form", :locals => {:f => f}) %>
    <p>
        <%= link_to("Back", preference_forms_path, class: "btn btn-secondary my-1") %>
        <%= f.submit("Add Questions", class: "btn btn-primary my-1") %>
    </p>
<% end %>