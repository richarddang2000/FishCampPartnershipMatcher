
<h1 class="my-4">Edit Question</h1>
<%= form_for(@question) do |f| %>
  <%= render(:partial => "form", :locals => {:f => f,:form_id => @form_id}) %>
  <p>
    <%= link_to("Back", questions_path(:form_id => @form_id), class: "btn btn-secondary mt-2 mb-2") %>
    <%= f.submit("Create Question", class: "btn btn-success") %>
  </p>
<% end %>