<% flash.each do |key, value| %>
  <div class="alert alert-<%= key %>"><%= value %></div>
<% end %>

<h1>Questions</h1>
<%= link_to("Create New Question", new_question_path, class: "btn btn-success mt-2 mb-4") %>


<% @questions.each do |question| %>
  <%= render(:partial => "question_index", :locals => {:question => question}) %>
<% end %>
